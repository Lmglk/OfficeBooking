{
    "name": "officebookingfrontend",
    "version": "0.0.3",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "prod": "ng build --prod --output-hashing none",
        "test": "ng test",
        "coverage": "ng test --coverage",
        "mock": "mockserver -p 9000 -m mockserver",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@ngrx/effects": "^7.2.0",
        "@ngrx/store": "^7.2.0",
        "@ngrx/store-devtools": "^7.2.0",
        "core-js": "^2.5.4",
        "ngx-toastr": "^10.0.2",
        "rxjs": "~6.3.3",
        "tslib": "^1.9.0",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-builders/jest": "^7.3.1",
        "@angular-devkit/build-angular": "~15.0.0",
        "@angular/cli": "~7.3.1",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@ngrx/schematics": "^7.2.0",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "faker": "^4.1.0",
        "husky": "^1.3.1",
        "jest": "^23.6.0",
        "jest-preset-angular": "^6.0.2",
        "mockserver": "^3.0.0",
        "prettier": "^1.16.4",
        "pretty-quick": "^1.10.0",
        "protractor": "~5.4.0",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "typescript": "~3.2.2"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm test && npm run prod && pretty-quick --staged"
        }
    },
    "jest": {
        "preset": "jest-preset-angular",
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts"
    }
}
